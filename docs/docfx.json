{
  "build": {
    "markdownEngineName": "markdig",
    "content": [
      {
        "files": [
          "**/*.md", 
          "**/*.yml"
        ],
        "group": "all",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "**/_data/**",
          "**/_shared/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif",
          "**/*.mp4",
          "**/*.svg"
        ],
        "group": "all",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "**/_data/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "uhfHeaderId": "MSDocsHeader-AzureDevOps",
      "ms.topic": "conceptual",
      "breadcrumb_path": "~/breadcrumb/toc.yml",
      "extendBreadcrumb": true,
      "searchScope": ["Azure DevOps Services"],
      "author": "thomps23",
      "ms.author": "jillfra",
      "ms.date": "09/05/2018",
      "ms.prod": "devops",
      "ms.version": "ALM",
      "MSHAttr.msprod": "ms.prod:ALM",
      "ms.prodfamily": "ALM",
      "feedback_system": "GitHub",
      "feedback_github_repo": "MicrosoftDocs/vsts-docs",
      "feedback_product_url": "https://developercommunity.visualstudio.com/spaces/21/index.html",
      "_op_documentIdPathDepotMapping": {
        "./": {
          "depot_name": "MSDN.team-services",
          "folder_relative_path_in_docset": "./"
        }
      }
    },
    "fileMetadata": {
      "titleSuffix": {
        "pipelines/**/**.md": "Azure Pipelines",
        "test/**/**.md": "Azure Test Plans"
      },
      "monikerRange": {
        "report/admin/**": "<= azure-devops-2019",
        "report/analytics/**": ">= azure-devops-2019",
        "report/excel/**": "<= tfs-2017",
        "report/extend-analytics/**": ">= azure-devops-2019",
        "report/powerbi/**": ">= azure-devops-2019",
        "report/sql-reports/**": "<= azure-devops-2019",
        "reference/tfs-ps-sync/**": "<= tfs-2015",
        "reference/witadmin/**": "<= azure-devops-2019",
        "reference/mapping/**": "<= azure-devops-2019"
      },
      "ROBOTS":{
        "integrate/previous-apis/**": "NOINDEX,NOFOLLOW"
      }
    },
    "template": [],
    "dest": "team-services",
    "groups": {
      "all": {
        "dest": "all",
        "moniker_range": "azure-devops || >= tfs-2013 <= tfs-2018 || >= azure-devops-2019"
      }
    }
  }
}
