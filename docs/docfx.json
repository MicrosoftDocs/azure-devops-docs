{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md", "**/*.yml"
        ],
        "group": "all",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif",
          "**/*.mp4",
          "**/*.svg"
        ],
        "group": "all",
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "uhfHeaderId": "MSDocsHeader-TeamServices",
      "ms.topic": "conceptual",
      "breadcrumb_path": "~/breadcrumb/toc.yml",
      "extendBreadcrumb": true,
      "searchScope": ["VSTS", "Visual Studio Team Services", "TFS", "Team Foundation Server"],
      "author": "erickson-doug",
      "ms.author": "douge",
      "ms.date": "07/13/2017",
      "ms.prod": "vs-devops-alm",
      "ms.version": "ALM",
      "MSHAttr.msprod": "ms.prod:ALM",
      "ms.prodfamily": "ALM",
      "feedback_system": "GitHub",
      "feedback_github_repo": "MicrosoftDocs/vsts-docs",
      "feedback_product_url": "https://developercommunity.visualstudio.com/spaces/21/index.html"  
    },
    "fileMetadata": {},
    "template": [],
    "dest": "team-services",
    "groups": {
      "all": {
        "dest": "all",
        "moniker_range": "vsts || >= tfs-2013 <= tfs-2018"
      }
    }
  }
}
