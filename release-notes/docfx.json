{
  "build": {
    "markdownEngineName": "markdig",
    "content": [
      {
        "files": [
          "**/*.md",
          "**/**.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "**/_shared/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [
      {
        "files": [
         "**/*.png",
          "**/*.jpg",
          "**/*.gif"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "ms.subservice": "azure-devops-release-notes",
      "uhfHeaderId": "MSDocsHeader-AzureDevOps",
      "ms.topic": "article",
      "breadcrumb_path": "~/breadcrumb/toc.yml",
      "extendBreadcrumb": true,
      "author": "gloridelmorales",
      "ms.author": "glmorale",
      "ms.manager": "mijacobs",
      "ms.service": "azure-devops",
      "ms.version": "ALM",
      "MSHAttr.msprod": "ms.prod:devops",
      "ms.prodfamily": "ALM",
      "hide_comments": "true",
      "_op_documentIdPathDepotMapping": {
        "./": {
          "depot_name": "VS.release-notes",
          "folder_relative_path_in_docset": "./"
        }
      }
    },
    "fileMetadata": {
      "ms.author": {
        "docswhatsnew/**": "sdanie"
      },
      "author": {
        "docswhatsnew/**": "steved0x"
      }
    },
    "template": [],
    "dest": "release-notes"
  }
}
